<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Freigabe" table="freigabe">
		<meta attribute="class-description">
			This class contains the freigabe detail.
		</meta>
		<id name="freigabeId" type="int" column="freigabeId">
			<generator class="native" />
		</id>
		<property name="userId" column="userId" type="int" />
		<property name="beuteId" column="beuteId" type="int" />
		<property name="rechtenummer" column="rechtenummer" type="int" />
	</class>
</hibernate-mapping>
